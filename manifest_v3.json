{
  "name": "FL Response Sounder",
  "description": "Simple FL API request & response sniffer.",
  "version": "1.1.0",
  "manifest_version": 3,
  "host_permissions": [
      "https://www.fallenlondon.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.fallenlondon.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_icon": {
      "16": "/images/fl-mystery-sorter-icon-16.png",
      "32": "/images/fl-mystery-sorter-icon-32.png",
      "48": "/images/fl-mystery-sorter-icon-48.png",
      "128": "/images/fl-mystery-sorter-icon-128.png"
    }
  },
  "icons": {
    "16": "/images/fl-mystery-sorter-icon-16.png",
    "32": "/images/fl-mystery-sorter-icon-32.png",
    "48": "/images/fl-mystery-sorter-icon-48.png",
    "128": "/images/fl-mystery-sorter-icon-128.png"
  },
  "web_accessible_resources": []
}